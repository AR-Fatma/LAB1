<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".LogoSelectorActivity">

    <ImageView
        android:id="@+id/teamid02"
        android:layout_width="89dp"
        android:layout_height="97dp"
        android:layout_marginTop="48dp"
        android:onClick="setTeamIcon"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:srcCompat="@drawable/ic_logo_02" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="Select a new Logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/teamid01"
        android:layout_width="89dp"
        android:layout_height="97dp"
        android:layout_marginTop="48dp"
        android:onClick="setTeamIcon"
        app:layout_constraintEnd_toStartOf="@+id/teamid02"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:srcCompat="@drawable/ic_logo_01" />

    <ImageView
        android:id="@+id/teamid00"
        android:layout_width="89dp"
        android:layout_height="97dp"
        android:layout_marginTop="24dp"
        android:onClick="setTeamIcon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.458"
        app:layout_constraintStart_toEndOf="@+id/teamid05"
        app:layout_constraintTop_toBottomOf="@+id/teamid03"
        app:srcCompat="@drawable/ic_logo_00" />

    <ImageView
        android:id="@+id/teamid05"
        android:layout_width="89dp"
        android:layout_height="97dp"
        android:layout_marginTop="24dp"
        android:onClick="setTeamIcon"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/teamid02"
        app:srcCompat="@drawable/ic_logo_05" />

    <ImageView
        android:id="@+id/teamid04"
        android:layout_width="89dp"
        android:layout_height="97dp"
        android:layout_marginTop="24dp"
        android:onClick="setTeamIcon"
        app:layout_constraintEnd_toStartOf="@+id/teamid05"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teamid01"
        app:srcCompat="@drawable/ic_logo_04" />

    <ImageView
        android:id="@+id/teamid03"
        android:layout_width="89dp"
        android:layout_height="97dp"
        android:layout_marginTop="48dp"
        android:onClick="setTeamIcon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toEndOf="@+id/teamid02"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:srcCompat="@drawable/ic_logo_03" />

</androidx.constraintlayout.widget.ConstraintLayout>